# Staging environment variables
# Copy this to .env.staging on your staging server and adjust as needed

# Frontend URL (staging subdomain)
FRONTEND_URL="https://staging.tomandrieu.com"

# API URL (staging API subdomain)
API_URL="https://api.staging.tomandrieu.com"

# Docker registry
CI_REGISTRY_IMAGE=tomandrieu

# App version tag for staging
APP_VERSION=staging

# Ports
FRONTEND_PORT=4200
BACKEND_PORT=3000
